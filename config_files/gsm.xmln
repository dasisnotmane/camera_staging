<?xml version='1.0' encoding='utf8'?>
<Config>
  <ppp_serial_device access="f" label="{{Serial device}}">ttyUSB0</ppp_serial_device>
  <gsm_apn_provider access="s" label="{{APN provider}}">dayum</gsm_apn_provider>
  <ETH-mode access="s" disable="DHCP:ETH-ipaddress,ETH-mask,ETH-gateway,ETH-dns1,ETH-dns2" label="{{Ethernet connection mode}}" options="DHCP|Static IP" type="list">DHCP</ETH-mode>
  <ETH-ipaddress access="s" label="{{IP Address}}">0.0.0.0</ETH-ipaddress>
  <ETH-mask access="s" label="{{IP Network Mask}}">0.0.0.0</ETH-mask>
  <ETH-gateway access="s" label="{{Default Gateway}}">0.0.0.0</ETH-gateway>
  <ETH-dns1 access="s" label="{{DNS Server (primary)}}">0.0.0.0</ETH-dns1>
  <ETH-dns2 access="s" label="{{DNS Server (secondary)}}">0.0.0.0</ETH-dns2>
  <PPP-Backup access="s" app="spcam500" label="{{Use GSM connection as backup only}}" type="boolean">false</PPP-Backup>
  <PPP-Backup-Restore-Timeout access="s" app="spcam500" label="{{In case of failure, try to restore Ethernet connection each (in minutes)}}" range="10..480" type="number">60</PPP-Backup-Restore-Timeout>
</Config>
